# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Required: Google Gemini API Key
# Get yours from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Database configuration
# If not provided, app will use in-memory storage
DATABASE_URL=postgresql://username:password@localhost:5432/jobmatch_pro

# Development settings
NODE_ENV=development
PORT=5000

# Optional: Additional configuration
MAX_FILE_SIZE=10485760  # 10MB in bytes
ALLOWED_FILE_TYPES=application/vnd.openxmlformats-officedocument.wordprocessingml.document